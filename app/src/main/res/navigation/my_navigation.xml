<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_navigation"
    app:startDestination="@id/jobListing">
    <fragment
        android:id="@+id/jobListing"
        android:name="com.example.careerhunt.JobListing"
        android:label="fragment_job_listing"
        tools:layout="@layout/fragment_job_listing" >
        <action
            android:id="@+id/action_jobListing_to_addJob"
            app:destination="@id/addJob" />
    </fragment>
    <fragment
        android:id="@+id/addJob"
        android:name="com.example.careerhunt.AddJob"
        android:label="fragment_add_job"
        tools:layout="@layout/fragment_add_job" />
</navigation>